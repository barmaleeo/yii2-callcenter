{
  "name": "barmaleeo/yii2-callcenter",

  "type": "yii2-extension",

  "description": "The Callcenter engine for the Yii framework",
  "keywords": ["yii2", "callcenter"],
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?labels=ext%3Aimagine",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "authors": [
    {
      "name": "Barmaleeo"
    }
  ],

  "require": {
    "yiisoft/yii2": "~2.0.0",
    "npm-asset/sip.js": "0.11.6",
    "npm-asset/react": "*"
  },

  "autoload": {
    "psr-4": {
      "barmaleeo\\callcenter\\": "src\\"
    }
  },
  "repositories" : {
    "type": "composer",
    "url": "https://asset-packagist.org"
  },

  "extra": {
    "bootstrap": "barmaleeo\\callcenter\\CallcenterBootstrap",
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }

  }
}